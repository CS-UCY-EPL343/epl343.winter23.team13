                    isSidebarVisible = !isSidebarVisible;
                    updateSidebar();
                });

                toggleRentAnything.addEventListener('click', () => {
                    isSidebarVisible = false;
                    updateSidebar();
                });

                function updateSidebar() {
                    if (isSidebarVisible) {
                        sidebar.style.transform = 'translateX(0)';


