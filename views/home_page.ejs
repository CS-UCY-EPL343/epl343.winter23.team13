    <script>
        let currentIndex = 0;
        const slides = document.querySelectorAll('.slider figure');
        const container = document.getElementById('slider-container');
        const slider = document.querySelector('.slider');

        function nextSlide() {
            currentIndex = (currentIndex + 1) % slides.length;
            container.style.transform = `translateX(-${currentIndex * 100}%)`;
            setSliderBackground()
        }

        function prevSlide() {
            // To prevent sliding to the left
            if (currentIndex > 0) {
                currentIndex = (currentIndex - 1) % slides.length;
                container.style.transform = `translateX(-${currentIndex * 100}%)`;
                setSliderBackground()
            }
        }

        function setSliderBackground() {
            const currentImage = slides[currentIndex].querySelector('img').getAttribute('src');
            document.querySelector('.slider').style.backgroundImage = `url('${currentImage}')`;
        }

        setInterval(nextSlide, 3000); // Change slide every 2 secs
    </script>
