  <div class="flex h-screen shrink-0 items-center justify-center z-10">
    <form method="post" action="/register"
      class="mb-4 w-2/5  px-2 pb-8 pt-6 shadow-2xl backdrop-blur-md font-roboto rounded-xl sm:min-w-full md:min-w-max">
      <div class="flex min-h-full flex-col justify-center px-6 py-12 lg:px-8">
        <div class="sm:mx-auto sm:w-full sm:max-w-sm">
          <h2 class="mt-10 text-center text-2xl font-bold leading-9 tracking-tight text-gray-900 ">Register for an
            account</h2>
        </div>

        <div class="mt-9 items-center justify-between sm:mx-auto sm:w-full sm:max-w-sm">
          <form class="space-y-6" method="post" action="/register">
            <div>
              <label for="name" class="block text-sm font-medium leading-6 text-gray-900">Your Name</label>
              <div class="mt-2">
                <input id="name" name="name" type="text" required
                  class="block w-full rounded-full border-0 py-1.5 px-3 text-gray-900 shadow-sm placeholder:text-gray-200 placeholder:text-sm"
                  placeholder="Name" />
              </div>
            </div>

            <div>
              <label for="email" class="block text-sm font-medium leading-6 text-gray-900">Your Email</label>
              <div class="mt-2">
                <input id="email" name="email" type="email" required
                  class="block w-full rounded-full border-0 py-1.5 px-3 text-gray-900 shadow-sm placeholder:text-gray-200 placeholder:text-sm"
                  placeholder="email@example.com" />
              </div>
            </div>

            <div>
              <label for="password" class="block text-sm font-medium leading-6 text-gray-900">Password</label>
              <div class="mt-2">
                <input id="password" name="password" type="password" required
                  class="block w-full rounded-full border-0 py-1.5 px-3 text-gray-900 shadow-sm placeholder:text-gray-200 placeholder:text-sm"
                  placeholder="Password" />
              </div>
            </div>

            <div>
              <label for="confirm" class="block text-sm font-medium leading-6 text-gray-900">Confirm Password</label>
              <div class="mt-2">
                <input id="confirm" name="confirm" type="password" required
                  class="block w-full rounded-full border-0 py-1.5 px-3 text-gray-900 shadow-sm placeholder:text-gray-200 placeholder:text-sm"
                  placeholder="Confirm Password" />
              </div>
            </div>

            <div>
              <button type="submit"
                class="flex w-full justify-center rounded-full mt-6 bg-indigo-600 px-3 py-1.5 text-sm font-semibold leading-6 text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Register</button>
            </div>
          </form>

          <p class="mt-10 text-center text-sm text-black">
            Already a member?
            <a href="/login" class="hover:text-contrast font-semibold leading-6 text-white shadow-sm">Login
              here.</a>
          </p>
        </div>
      </div>
    </form>
    <div>
</body>

</html>